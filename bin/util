#!/bin/bash
# Main utility file - imports all utility functions

# Get the directory where the util command is located
UTIL_PATH="$(which util)"
SCRIPT_DIR="$(dirname "$UTIL_PATH")"
UTILS_DIR="$SCRIPT_DIR/utils"

# Source all utility functions
source "$UTILS_DIR/gittag.sh"
source "$UTILS_DIR/gitbranch.sh"
source "$UTILS_DIR/showfile.sh"
source "$UTILS_DIR/gitpush.sh"
